{
  "extends": ["react-app", "eslint:recommended"],
  "rules": {
    "no-undef": "warn",
    "no-undef-init": "warn",
    "no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],

    "react/jsx-uses-react": "warn",
    "react/jsx-uses-vars": "warn"
  }
}
